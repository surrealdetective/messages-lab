<html>
  <head>
    <meta charset="utf-8" />
    <title></title>
  </head>
  <body>
    <% @messages.each do |message| %>
      <div>
        <p>From: <%= message.sender %></br>
        To: <%= message.receiver %></br>
        Message: <%= message.content %></p>
      </div>
    <% end %>
    <a href="/reset"><b>DELETE ALL MESSAGES</b></a>
  </body>
</html>
